<div class="" id=" saved-outfits-content" role="tabpanel" aria-labelledby="saved-outfits-tab">
    <h2>Saved Outfits</h2>
    <div class="overflow-hidden">
        <button id="delete-outfit" class="overflow-hidden">Delete Outfit</button>
        <button id="add-to-calendar" class="overflow-hidden">Add to calender </button>
        <button id="remove-from-calendar" class="overflow-hidden">Remove from calender </button>
    </div>
    <!-- start content here for saved outfits -->

    <div id="outfits-container">
        {{#each outfits}}
        <div class="outfit-box">
            <p>{{dataValues.name}}</p>
            {{#each dataValues.items}}
            <div>
                <p> {{dataValues.name}}</p>
                <img height="100" width="100" src="{{dataValues.imageURL}}" />
            </div>
            {{/each}}
            <button class="select-outfit" name="{{dataValues.id}}"> Select Outfit </button>
        </div>
        {{/each}}
    </div>